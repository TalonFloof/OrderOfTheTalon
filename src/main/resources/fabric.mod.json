{
  "schemaVersion": 1,
  "id": "talon",
  "version": "${version}",

  "name": "Order of the Talon",
  "description": "Total Revamp mod for Beta 1.7.3",
  "authors": [
    "TalonFloof"
  ],
  "contact": {
    "homepage": "https://glass-launcher.net/",
    "sources": "https://github.com/calmilamsy/stationapi-example-mod"
  },

  "license": "All Rights Reserved",
  "icon": "assets/talon/icon.png",

  "environment": "*",
  "entrypoints": {
    "stationapi:event_bus": [
      "sh.talonfloof.orderofthetalon.events.init.AchievementListener",
      "sh.talonfloof.orderofthetalon.events.init.BlockListener",
      "sh.talonfloof.orderofthetalon.events.init.ItemListener",
      "sh.talonfloof.orderofthetalon.events.init.PlayerHandlerListener",
      "sh.talonfloof.orderofthetalon.events.init.RecipeListener",
      "sh.talonfloof.orderofthetalon.events.init.BiomeListener"
    ],
    "stationapi:event_bus_client": [
      "sh.talonfloof.orderofthetalon.events.init.KeyBindingListener",
      "sh.talonfloof.orderofthetalon.events.init.TextureListener",
      "sh.talonfloof.orderofthetalon.events.ingame.KeyPressedListener"
    ],
    "stationapi:event_bus_server": [
      "sh.talonfloof.orderofthetalon.events.init.ServerInitListener"
    ],
    "spasm:transformer": [
      "sh.talonfloof.orderofthetalon.asm.MaxStackCountTransformer"
    ]
  },
  "mixins": [
    "talon.mixins.json"
  ],

  "depends": {
    "minecraft": "1.0.0-beta.7.3"
  }
}
